n\r\nhtml {\r\n  height: 100%;\r\n  padding-left: 0 ;\r\n}\r\n\r\nbody {\r\n  height: inherit;\r\n  padding-left: 0 !important;\r\n  margin-left: 0 !important;\r\n}\r\n\r\n/* Aside */\r\naside.pushed {\r\n  width: 30%;\r\n  transition: width 2s;\r\n}\r\n\r\naside {\r\n  height: inherit;\r\n  float: left;\r\n  margin-left: 0;\r\n  display: inline-flex;\r\n  flex-direction: column;\r\n  justify-content: flex-start;\r\n  width: 0%;\r\n  background-color: $base03;\r\n  transition: width 2s;\r\n  background-color: $base02;\r\n  overflow: hidden;\r\n}\r\n\r\n\r\n#aside-menu-button {\r\n  display:none;\r\n  align-self: flex-end;\r\n  margin-right: 1.2rem;\r\n  z-index: 3000;\r\n}\r\n\r\naside > #aside-link-spacer {\r\n  display: flex;\r\n  font-family: 'Titillium Web', sans-serif;\r\n  font-size: 1.3rem;\r\n  text-indent: 1.5rem;\r\n  flex-direction: column;\r\n  justify-content: flex-end; \r\n  padding: 3rem;\r\n  padding-left: 1rem;\r\n}\r\n\r\naside.pushed > #aside-link-spacer {\r\n  border: none;\r\n  padding: 3rem;\r\n  padding-left: 1rem;\r\n}\r\n\r\n#aside-link-spacer > div {\r\n\r\n}\r\n\r\naside > div {\r\n  overflow: hidden;\r\n  width: 100rem;\r\n}\r\n\r\n/* Wrapper */\r\n.wrapper {\r\n  transition: width 1s;\r\n}\r\n\r\n/* Header */\r\n\r\nheader {\r\n  display: flex;\r\n  align-self: center;\r\n  text-align: center;\r\n}\r\n\r\n\r\nheader > #nav-button-icons {\r\n  justify-content: flex-end;\r\n}\r\n\r\nheader > #list-name {\r\n  display: flex;\r\n  align-self: center;\r\n  text-align: center;\r\n}\r\n\r\n#add-task-input {\r\n  flex-grow: 1;\r\n}\r\n\r\n/* Section */\r\n\r\nsection {\r\n  display: flex;\r\n}\r\n\r\nsection > ul {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  flex-direction: column;\r\n  list-style-type: none;\r\n}\r\n\r\nsection > ul > li {\r\n  display: flex;\r\n}\r\n\r\n#task-area > ul {\r\n  width: 10000px;\r\n}\r\n\r\n#task-list {\r\n  width: max-content;\r\n}\r\n\r\n#task-area > ul > li > button {\r\n  align-self: flex-end;\r\n  padding-inline-end: 1rem;\r\n}\r\n\r\n#task-delete-button {\r\n  background-color: transparent;\r\n  border-style: none;\r\n  border-color: none;\r\n}\r\n\r\n#task-edit-button {\r\n  background-color: transparent;\r\n  border-style: none;\r\n  border-color: none;\r\n}\r\n\r\n/* General Theming */\r\ninput {\r\n  background-color: $base02;\r\n  border: none;\r\n}\r\n\r\n.btn {color: $base01}\r\n\r\n.btn:hover, .btn:focus, .btn:active, .btn.active, .open>.dropdown-toggle.btn {\r\n    color: $orange;\r\n    box-shadow: none;\r\n}\r\n\r\n\r\n/* When screen size is too small */\r\n@media (max-width: 767.98px) {\r\n\r\n  aside {\r\n    position: absolute;\r\n    width: 0%;\r\n    z-index: 2000;\r\n  }\r\n\r\n  aside.pushed {\r\n    width: 50%;\r\n  }\r\n\r\n\r\n  /* Display inner menu button */\r\n  #aside-menu-button {\r\n    display: flex;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"3d35df76b85c9445a5f974cb13c37b3e","cacheData":{"env":{}}}